/* 固定头部搜索栏 */
.continer{
  padding-top: 100rpx;
}

/* 搜索框 */

/* 轮播图 */
.slide-image{
  width: 100%;
  height: 340rpx;
}

.menus{
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
}

.menus image{
  width: 128rpx;
  height: 140rpx
}

/* 楼层 */
.floor{
  margin-top: 20rpx;
}

.floor-title image{
  width: 750rpx;
  height: 60rpx;
}

.floor-content{
  padding: 20rpx;
  display: flex;
  justify-content: space-around;
}

.floor-content-left image{
  width: 232rpx;
  height: 386rpx;
}

.floor-content-right{
  flex: 1;
  margin-left: 10rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.floor-content-right image{
    width: 233rpx;
    height: 188rpx;
}